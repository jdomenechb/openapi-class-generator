{
  "name": "jdomenechb/openapi-class-generator",
  "description": "Generate PHP classes from given OpenAPI contracts.",
  "type": "library",
  "license": "MIT",
  "version": "0.1.0-alpha",
  "authors": [
    {
      "name": "Jordi DomÃ¨nech",
      "email": "jdomenechbborges@gmail.com"
    }
  ],
  "bin": [
    "bin/ocg"
  ],
  "autoload": {
    "psr-4": {
      "Jdomenechb\\OpenApiClassGenerator\\" : "src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "symfony/console": "^4.3",
    "cebe/php-openapi": "^1.2",
    "nette/php-generator": "^3.2",
    "symfony/finder": "^4.3",
    "doctrine/inflector": "^1.3",
    "psr/http-client": "^1.0",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/filesystem": "^4.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpstan/phpstan": "^0.11.15"
  },
  "scripts": {
    "test": "bin/phpunit",
    "cs": "vendor/bin/php-cs-fixer fix",
    "stan": "vendor/bin/phpstan analyze"
  }
}
